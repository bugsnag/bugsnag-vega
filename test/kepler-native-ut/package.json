{
  "name": "@bugsnag/kepler-native-ut",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist",
    "test": "jest --colors",
    "lint": "eslint src tst --ext .ts,.tsx",
    "lint:fix": "eslint src tst --ext .ts,.tsx --fix",
    "build:js": "tsc -p tsconfig.json",
    "build:native": "kepler build -b Debug -b Release",
    "build:module": "npm-run-all build:js build:native",
    "prepack": "npm-run-all build:module",
    "local-npm:publish": "npm publish --registry http://0.0.0.0:5539"
  },
  "dependencies": {
    "@amzn/keplerscript-turbomodule-api": "^1.0.0"
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.72.2",
    "@types/jest": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "babel-jest": "^28.0.0",
    "eslint": "^8.12.0",
    "jest": "^28.0.0",
    "metro-react-native-babel-preset": "^0.76.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.0",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "kepler": {
    "projectType": "turbomodule",
    "targets": [
      "tv"
    ],
    "api": 0.1,
    "keplerInitializationSharedLibrary": "BugsnagNativeUT",
    "autolink": {
      "BugsnagNativeUT": {
        "libraryName": "libBugsnagNativeUT.so",
        "linkDynamic": true,
        "provider": "application",
        "components": [],
        "turbomodules": [
          "BugsnagNativeUT"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "http://0.0.0.0:5539"
  }
}
