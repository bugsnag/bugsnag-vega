{
  "name": "bugsnag-kepler",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test:lint": "eslint --max-warnings=0 .",
    "test:unit": "jest",
    "local-npm:publish": "lerna publish \"$VERSION_IDENTIFIER\" --yes --force-publish --exact --no-push --no-git-reset --no-git-tag-version --registry 'http://0.0.0.0:5539'",
    "local-npm:start": "verdaccio --config local-npm-config.yml --listen 0.0.0.0:5539"
  },
  "devDependencies": {
    "@react-native/babel-preset": "^0.73.19",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "lerna": "^8.0.1",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "typescript": "^5.3.3",
    "verdaccio": "^5.29.0"
  }
}
