{
  "name": "bugsnag-kepler",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test:lint": "eslint --max-warnings=0 .",
    "test:unit": "jest",
    "local-npm:publish": "lerna publish \"$VERSION_IDENTIFIER\" --yes --force-publish --exact --no-push --no-git-reset --no-git-tag-version --registry 'http://0.0.0.0:5539'",
    "local-npm:start": "verdaccio --config local-npm-config.yml --listen 0.0.0.0:5539"
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.72.2",
    "@types/jest": "^28.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^28.0.0",
    "lerna": "^7.4.2",
    "metro-react-native-babel-preset": "^0.76.5",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "verdaccio": "^5.29.0"
  }
}
