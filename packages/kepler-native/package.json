{
  "name": "@bugsnag/kepler-native",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "build:js": "tsc -p tsconfig.json",
    "build:native": "kepler build -b Debug -b Release",
    "build:module": "npm-run-all build:js build:native",
    "prepack": "npm-run-all build:module"
  },
  "dependencies": {
    "@amzn/keplerscript-turbomodule-api": "^1.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "kepler": {
    "projectType": "turbomodule",
    "targets": [
      "tv"
    ],
    "api": 0.1,
    "keplerInitializationSharedLibrary": "BugsnagKeplerNative",
    "autolink": {
      "BugsnagKeplerNative": {
        "libraryName": "libBugsnagKeplerNative.so",
        "linkDynamic": true,
        "provider": "application",
        "components": [],
        "turbomodules": [
          "BugsnagKeplerNative"
        ]
      }
    }
  }
}
