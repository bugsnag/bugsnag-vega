{
  "name": "@bugsnag/kepler-native",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist",
    "lint": "eslint src tst --ext .ts,.tsx",
    "lint:fix": "eslint src tst --ext .ts,.tsx --fix",
    "compile": "tsc -p tsconfig-build.json",
    "build": "npm run compile",
    "release": "npm-run-all lint test compile"
  },
  "dependencies": {
    "@amzn/keplerscript-turbomodule-api": "^1.0.0",
    "react-native-tscodegen-types": "0.66.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "metro-react-native-babel-preset": "^0.76.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "kepler": {
    "projectType": "turbomodule",
    "targets": [
      "tv"
    ],
    "api": 0.1,
    "keplerInitializationSharedLibrary": "BugsnagKeplerNative",
    "autolink": {
      "BugsnagKeplerNative": {
        "libraryName": "libBugsnagKeplerNative.so",
        "linkDynamic": true,
        "provider": "application",
        "components": [],
        "turbomodules": [
          "BugsnagKeplerNative"
        ]
      }
    }
  }
}
